{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "setupFilesAfterEnv": ["<rootDir>/jest.setup.ts"],
  "moduleFileExtensions": ["ts", "js", "json"],
  "transformIgnorePatterns": ["<rootDir>/node_modules/"],
  "testMatch": [
    "**/?(*.)+(spec|test).ts"
  ]
}
